{
  "name": "api-public",
  "version": "1.0.0",
  "description": "FX1 Public API",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --runInBand --passWithNoTests --forceExit",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "generate:nexus": "ts-node --transpile-only src/nexus/generate.ts",
    "generate:graphql": "graphql-codegen --config graphql-codegen.yml",
    "dev": "ts-node-dev ./src/index.ts",
    "start": "npx ts-node src/index.ts",
    "healthcheck": "npx ts-node src/healthcheck.ts",
    "seed:game": "npx ts-node src/runners/CreateOrUpdateGamesFromAUWT.ts",
    "seed:fakeGame": "npx ts-node src/runners/CreateOrUpdateGamesFromAUWT.ts --fake",
    "seed:gamePartner": "npx ts-node src/runners/ExtractExistingGamePartners.ts",
    "seed:gameLockerRooms": "npx ts-node src/runners/CreateLockerRoomForExistingGames.ts"
  },
  "author": "Mari Joyce S. Mamac (mari@fx1.io)",
  "license": "MIT",
